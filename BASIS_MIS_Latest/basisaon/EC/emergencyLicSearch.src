serialNbrRecordSet! = bbjapi!.createSQLRecordSet(ourDbUrl$,"",sql$)

recordCount = 0
snList! = bbjapi!.makeVector()
authCodeList! = bbjapi!.makeVector()

if serialNbrRecordSet!.isEmpty() then

