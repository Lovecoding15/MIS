      if xx__win_id$="101" then return "BUYNOW:C(1*=0):ID=1 TYPE=11 X=15 Y=80 W=90 H=25:,EXIT:C(1*=0):ID=2 TYPE=11 X=115 Y=80 W=90 H=25:,PONUMBER_LBL:C(64*=0):ID=101 TYPE=17 X=15 Y=30 W=90 H=25:,PONUMBER:C(64*=0):ID=102 TYPE=16 X=115 Y=30 W=90 H=25:"
      return ""
