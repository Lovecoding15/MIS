0010 REM "SAX - Rebuild Sales Anaylis From Invoice History (Rebuild Overlay)
0020 REM "Program SAX.BB
0022 REM "+-----------------------------------------------------------+"
0024 REM "| ADD+ON Software, Inc. Advantage/V Version 6.0.0 - 15May96 |"
0026 REM "|      Copyright (c) 1996 Open Systems Holdings Corp.       |"
0028 REM "|                  All Rights Reserved                      |"
0030 REM "+-----------------------------------------------------------+"
0040 REM 
0085 SETESC 9000
0090 SETERR 9000
0200 REM " --- IOLists
0210 ART03A: IOLIST A0$,A[ALL]
0220 ART13A: IOLIST W0$,W1$(1),W[ALL]
0250 ARM01A: IOLIST *,X4$(1)
0255 ARM02A: IOLIST *,X3$(1)
0270 ARM10E: IOLIST Y0$
0340 IVM01A: IOLIST *,*,X2$(1)
0350 IVM02A: IOLIST *,T1$(1)
0360 IVW05A: IOLIST *,W[0]
0370 SAM01A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0372 SAM02A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0374 SAM03A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0376 SAM04A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0378 SAM05A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0380 SAM06A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0382 SAM07A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0384 SAM08A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0386 SAM09A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0388 SAM10A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0390 SAM11A: IOLIST X0$,X[ALL],Y[ALL],Z[ALL]
0395 SAM21A: IOLIST X0$
0500 REM " --- Initializations
0510 PRECISION IVPRECISION
0520 DIM A[10],W1$(64),W[14]
0700 REM " --- Background
0750 CALL "SYC.NB","Updating",LEN(CUSTOMASK$)+8,COLUMN
0850 REM " --- Disallow 'M'enu Option In Error Routine
0860 LET EXIT_CTRL=1
1000 REM " --- Position File
1090 READ (ART03_DEV,KEY=N0$,DOM=1100)
1100 REM " --- Main read
1120 LET K0$=KEY(ART03_DEV)
1140 READ (ART03_DEV)IOL=ART03A
1150 IF POS(N0$=K0$)<>1 THEN GOTO 4000
1190 PRINT @(COLUMN,11),FNF$(A0$(5,CUSTLEN),CUSTOMASK$)," ",A0$(11,7),
1500 REM " --- Position Invoice History Detail File
1510 READ (ART13_DEV,KEY=K0$(1,17),DOM=1600)
1600 REM " --- Read next line item
1610 LET K$=KEY(ART13_DEV,END=3900)
1620 IF K$(1,17)<>K0$(1,17) THEN GOTO 3900
1630 READ (ART13_DEV)IOL=ART13A
1640 IF LF$="Y" THEN FIND (IVW05_DEV,KEY=W0$(1,17)+W0$(33)+W0$(18,3),ERR=1700)IOL=IVW05A
1700 REM " --- Addition Reads
1710 DIM X3$(64),X4$(325)
1720 FIND (ARM01_DEV,KEY=N0$+A0$(5,6),DOM=1730)IOL=ARM01A
1730 FIND (ARM02_DEV,KEY=N0$+A0$(5,6)+"  ",DOM=2100)IOL=ARM02A
2100 REM " --- Get Line Type
2120 IF W0$(21,1)=K2$ THEN GOTO 2200
2130 LET K2$=W0$(21,1)
2140 DIM Y0$(32)
2150 FIND (ARM10_DEV,KEY=N0$+"E"+W0$(21,1),DOM=2200)IOL=ARM10E
2400 REM " --- Update Sales Analysis
2410 IF POS(Y0$(25,1)="SNPO")=0 THEN GOTO 3500
2420 IF POS(Y0$(25,1)="NPO") AND Y0$(32,1)="N" THEN GOTO 3500
2430 DIM X2$(62),T1$(64)
2450 FIND (IVM01_DEV,KEY=N0$+W0$(33,20),DOM=2460)IOL=IVM01A
2460 FIND (IVM02_DEV,KEY=N0$+W0$(31,2)+W0$(33,20),DOM=2470)IOL=IVM02A
2470 IF X2$(24,1)="W" THEN LET VENDOR$=T1$(22,6) ELSE LET VENDOR$=X2$(12,6)
2480 CALL "SYC.PB",N0$,SYS01_DEV,A0$(24,3),PERIOD$,YEAR$
2490 LET PERIOD=NUM(PERIOD$)
2500 REM " --- Customer
2505 IF OPTIONS$(1,1)<>"Y" OR POS(X3$(11,1)="PI")=0 THEN GOTO 2600
2510 LET SEGS=3
2515 DIM SEG_END[SEGS],X0$(33)
2520 LET SEG_END[1]=11,SEG_END[2]=14,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2530 LET X0$(1)=N0$+YEAR$+A0$(5,6)+W0$(23,3)+W0$(33,20),SA_DEV=SAM01_DEV
2535 IF X3$(11,1)="P" OR LEVPARAMS$(1,1)="P" THEN LET SEGS=2
2540 IF LEVPARAMS$(1,1)="C" THEN LET SEGS=1
2545 GOSUB 6000
2550 REM " --- Product/Item/Customer Xref
2560 LET X0$=N0$+YEAR$+W0$(23,3)+W0$(33,20)+A0$(5,6)
2570 IF X3$(11,1)="P" OR LEVPARAMS$(1,1)="P" THEN LET X0$(8,20)=""
2580 IF LEVPARAMS$(1,1)="C" THEN LET X0$(5,23)=""
2590 WRITE (SAM21_DEV,KEY=X0$)IOL=SAM21A
2600 REM " --- Territory
2605 IF OPTIONS$(2,1)<>"Y" OR POS(X2$(18,1)="PI")=0 THEN GOTO 2650
2610 LET SEGS=3
2615 DIM SEG_END[SEGS],X0$(30)
2620 LET SEG_END[1]=8,SEG_END[2]=11,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2630 LET X0$(1)=N0$+YEAR$+A0$(72,3)+W0$(23,3)+W0$(33,20),SA_DEV=SAM02_DEV
2635 IF X2$(18,1)="P" OR LEVPARAMS$(2,1)="P" THEN LET SEGS=2
2640 IF LEVPARAMS$(2,1)="T" THEN LET SEGS=1
2645 GOSUB 6000
2650 REM " --- Salesperson
2655 IF OPTIONS$(3,1)<>"Y" OR POS(X2$(18,1)="PI")=0 THEN GOTO 2700
2660 LET SEGS=3
2665 DIM SEG_END[SEGS],X0$(30)
2670 LET SEG_END[1]=8,SEG_END[2]=11,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2680 LET X0$(1)=N0$+YEAR$+A0$(59,3)+W0$(23,3)+W0$(33,20),SA_DEV=SAM03_DEV
2685 IF X2$(18,1)="P" OR LEVPARAMS$(3,1)="P" THEN LET SEGS=2
2690 IF LEVPARAMS$(3,1)="S" THEN LET SEGS=1
2695 GOSUB 6000
2700 REM " --- Customer Type
2705 IF OPTIONS$(4,1)<>"Y" OR POS(X2$(18,1)="PI")=0 THEN GOTO 2750
2710 LET SEGS=3
2715 DIM SEG_END[SEGS],X0$(30)
2720 LET SEG_END[1]=8,SEG_END[2]=11,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2730 LET X0$(1)=N0$+YEAR$+X3$(34,3)+W0$(23,3)+W0$(33,20),SA_DEV=SAM04_DEV
2735 IF X2$(18,1)="P" OR LEVPARAMS$(4,1)="P" THEN LET SEGS=2
2740 IF LEVPARAMS$(4,1)="C" THEN LET SEGS=1
2745 GOSUB 6000
2750 REM " --- Ship-To
2755 IF OPTIONS$(5,1)<>"Y" OR POS(X3$(11,1)="PI")=0 THEN GOTO 2800
2760 LET SEGS=3
2765 DIM SEG_END[SEGS],X0$(36)
2770 LET SEG_END[1]=11,SEG_END[2]=17,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2780 LET X0$(1)=N0$+YEAR$+A0$(5,6)+A0$(102,6)+W0$(33,20),SA_DEV=SAM05_DEV
2785 IF X3$(11,1)="P" OR LEVPARAMS$(5,1)="P" THEN LET SEGS=2
2790 IF LEVPARAMS$(5,1)="C" THEN LET SEGS=1
2795 GOSUB 6000
2800 REM " --- Standard Industry Code
2805 IF OPTIONS$(6,1)<>"Y" OR POS(X2$(18,1)="PI")=0 OR FNP$(X4$(257,8))="" THEN GOTO 2850
2810 LET SEGS=3
2815 DIM SEG_END[SEGS],X0$(35)
2820 LET SEG_END[1]=9,SEG_END[2]=12,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2830 LET X0$(1)=N0$+YEAR$+X4$(257,8)+W0$(23,3)+W0$(33,20),SA_DEV=SAM06_DEV
2835 IF X2$(18,1)="P" OR LEVPARAMS$(6,1)="P" THEN LET SEGS=2
2840 IF LEVPARAMS$(6,1)="C" THEN LET SEGS=1
2845 GOSUB 6000
2850 REM " --- Product Type
2855 IF OPTIONS$(7,1)<>"Y" OR POS(X2$(18,1)="PI")=0 OR Y0$(25,1)="N" THEN GOTO 2900
2860 LET SEGS=2
2865 DIM SEG_END[SEGS],X0$(27)
2870 LET SEG_END[1]=8,SEG_END[2]=LEN(X0$)+1; REM "End of segment + 1
2880 LET X0$(1)=N0$+YEAR$+W0$(23,3)+W0$(33,20),SA_DEV=SAM07_DEV
2885 IF X2$(18,1)="P" OR LEVPARAMS$(7,1)="P" THEN LET SEGS=1
2895 GOSUB 6000
2900 REM " --- Warehouse
2905 IF OPTIONS$(8,1)<>"Y" OR POS(X2$(18,1)="PI")=0 THEN GOTO 2950
2910 LET SEGS=3
2915 DIM SEG_END[SEGS],X0$(29)
2920 LET SEG_END[1]=7,SEG_END[2]=10,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2930 LET X0$(1)=N0$+YEAR$+W0$(31,2)+W0$(23,3)+W0$(33,20),SA_DEV=SAM08_DEV
2935 IF X2$(18,1)="P" OR LEVPARAMS$(8,1)="P" THEN LET SEGS=2
2940 IF LEVPARAMS$(8,1)="W" THEN LET SEGS=1
2945 GOSUB 6000
2950 REM " --- Stocking Vendor
2955 IF OPTIONS$(9,1)<>"Y" OR POS(X2$(18,1)="PI")=0 OR NUM(VENDOR$(1,6),ERR=3000)=0 THEN GOTO 3000
2960 LET SEGS=3
2965 DIM SEG_END[SEGS],X0$(33)
2970 LET SEG_END[1]=11,SEG_END[2]=14,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
2980 LET X0$(1)=N0$+YEAR$+VENDOR$(1,6)+W0$(23,3)+W0$(33,20),SA_DEV=SAM09_DEV
2985 IF X2$(18,1)="P" OR LEVPARAMS$(9,1)="P" THEN LET SEGS=2
2990 IF LEVPARAMS$(9,1)="V" THEN LET SEGS=1
2995 GOSUB 6000
3000 REM " --- Distribution Code
3005 IF OPTIONS$(10,1)<>"Y" OR POS(X2$(18,1)="PI")=0 THEN GOTO 3050
3010 LET SEGS=3
3015 DIM SEG_END[SEGS],X0$(29)
3020 LET SEG_END[1]=7,SEG_END[2]=10,SEG_END[3]=LEN(X0$)+1; REM "End of segment + 1
3030 LET X0$(1)=N0$+YEAR$+A0$(66,2)+W0$(23,3)+W0$(33,20),SA_DEV=SAM10_DEV
3035 IF X2$(18,1)="P" OR LEVPARAMS$(10,1)="P" THEN LET SEGS=2
3040 IF LEVPARAMS$(10,1)="D" THEN LET SEGS=1
3045 GOSUB 6000
3050 REM " --- Non-Stock
3055 IF OPTIONS$(11,1)<>"Y" OR Y0$(25,1)<>"N" THEN GOTO 3500
3060 LET SEGS=2
3065 DIM SEG_END[SEGS],X0$(47)
3070 LET SEG_END[1]=8,SEG_END[2]=LEN(X0$)+1; REM "End of segment + 1
3080 LET X0$(1)=N0$+YEAR$+W0$(23,3)+W1$(1,40),SA_DEV=SAM11_DEV
3085 IF LEVPARAMS$(11,1)="P" THEN LET SEGS=1
3095 GOSUB 6000
3500 REM " --- Loop back for next line item
3590 GOTO 1600
3900 REM " --- Loop back for next invoice
3990 GOTO 1100
4000 REM " --- All done
4090 GOTO 9900
6000 REM " --- Write Sales Analysis Records
6005 LET X0$(SEG_END[SEGS])=""
6010 DIM X[12],Y[12],Z[12]
6020 FIND (SA_DEV,KEY=X0$,DOM=6030)IOL=SAM01A
6030 LET X[PERIOD-1]=X[PERIOD-1]+W[4]
6040 PRECISION 2
6050 LET Y[PERIOD-1]=Y[PERIOD-1]+W[0]*W[4]
6060 PRECISION IVPRECISION
6070 LET Z[PERIOD-1]=Z[PERIOD-1]+W[6]
6080 WRITE (SA_DEV,KEY=X0$)IOL=SAM01A
6090 RETURN 
8000 REM " --- Functions
8095 DEF FNP$(Q$)=CVS(Q$,2)
8200 REM " --- FNF$ AlphaNumeric Masking Function"
8210 DEF FNF$(Q1$,Q2$)
8220 IF Q2$="" THEN LET Q2$=FILL(LEN(Q1$),"0")
8230 RETURN STR(-NUM(Q1$,ERR=ALPHA_FNF):Q2$,ERR=ALPHA_FNF)
8240 ALPHA_FNF: 
8245 LET Q=1,Q0=0
8250 WHILE LEN(Q2$(Q))
8255 IF POS(Q2$(Q,1)="-()") THEN LET Q0=Q0+1 ELSE LET Q2$(Q,1)="X"
8260 LET Q=Q+1
8265 WEND
8270 IF LEN(Q1$)>LEN(Q2$)-Q0 THEN LET Q1$=Q1$(1,LEN(Q2$)-Q0)
8280 RETURN STR(Q1$:Q2$)
8290 FNEND
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9210 SETERR 9000
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9900 RUN "SYS.AA"
9999 END
