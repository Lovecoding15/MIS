2230 PRINT (7)@(8),"Type of Sale: ",@(22),A0$(6,3)," ",TYPE_DESC$
2235 PRINT (7)@(8),"Discountable: ",@(22),A1$(1,1),"  Distribution Code: ",A1$(2,2)
2240 PRINT (7)'LF',@(6),"Minimum Users",@(20),"Maximum Users",@(54),"Price per User",'LF'
2250 LET L=L+6
2260 FOR X=0 TO 9
2265 IF L>L9 THEN GOSUB 5000
