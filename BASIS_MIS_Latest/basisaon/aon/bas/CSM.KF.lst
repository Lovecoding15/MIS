2280 IF A[20+X]<>0 THEN PRINT (7)@(54),A[20+X]:M4$,
2285 PRINT (7)""
2287 LET L=L+1
2290 NEXT X
2300 PRINT (7)J$
2310 LET L=L+1,T0=T0+1
3900 REM 
