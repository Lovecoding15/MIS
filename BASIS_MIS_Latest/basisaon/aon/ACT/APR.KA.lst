0010 REM "APR - VENDOR LABEL PRINTING"
0020 REM "Program APR.KA"
0022 REM "+-----------------------------------------------------------+"
0024 REM "| ADD+ON Software, Inc. Advantage/V Version 6.0.0 - 15May96 |"
0026 REM "|      Copyright (c) 1996 Open Systems Holdings Corp.       |"
0028 REM "|                  All Rights Reserved                      |"
0030 REM "+-----------------------------------------------------------+"
0040 REM 
0050 BEGIN 
0080 SETESC 9000
0090 SETERR 9000
0100 REM "OPEN/LOCK FILES"
0110 LET FILES=3
0120 DIM FILES$[FILES],OPTIONS$[FILES],CHANNELS[FILES]
0130 LET FILES$[1]="APM-01",FILES$[2]="APM-04",FILES$[3]="SYS-01"
0160 CALL "SYC.DA",1,1,3,FILES$[ALL],OPTIONS$[ALL],CHANNELS[ALL],BATCH,STATUS
0170 IF STATUS>0 THEN GOTO 9900
0180 LET APM01_DEV=CHANNELS[1],APM04_DEV=CHANNELS[2],SYS01_DEV=CHANNELS[3]
0200 REM "IOLISTS"
0260 SYS01T: IOLIST X$,F0$,X$,X$,X$,F4$,F5$
0270 APS01A: IOLIST P0$,P1$,P2$,P3$,P4$,M0$,M1$,M2$,M3$
0400 REM "PARAMETERS"
0410 FIND (SYS01_DEV,KEY="T"+FID(0),DOM=9800)IOL=SYS01T
0420 LET N0$=F0$(16,2),N1$=F4$,N2$="AP",N3$=F5$
0430 FIND (SYS01_DEV,KEY=N0$+N2$+"00",DOM=9800)IOL=APS01A
0450 DIM P[2]
0480 LET P[0]=NUM(P2$(1,2)),P[1]=NUM(P2$(3,2)),P[2]=NUM(P3$(9,1))
0500 REM "INIT DATA"
0530 LET W0=NUM(P3$(10,2)),W1=NUM(P3$(14,2)),W2=NUM(P3$(12,2)),W3=1
0540 DIM W0$(W0*W1*4),W9$(W2,$0A$)
0550 LET M7$="00000"
0560 IF P[2]=9 THEN LET M7$="00000-0000"
0700 REM "BACKGROUND"
0710 FOR X=4 TO 14 STEP 2
0720 CALL "SYC.WC",1,0,80,0,0,X,0
0730 NEXT X
0740 PRINT 'SB',@(28,3),"Label Type:",@(21,5),"Labels Per Record:",@(24,7),"Restart (Y/N):",@(19,9),"Enter Starting Info:",@(21,11),"Enter Ending Info:",'SF'
1000 REM "TYPE"
1010 PRINT 'CF',
1015 LET I0=0
1020 LET V0$="S",V1$="KREC",V2$="N",V3$="NAZ",V4$="N=Numeric A=Alternate Sequence Z=Zip Code",V0=1,V1=40,V2=3
1040 GOSUB 7000
1045 IF V3=4 THEN GOTO 9900
1060 LET P7$=V$
1070 IF P7$="N" THEN LET R0=6,R0$="M",R3$=P1$
1080 IF P7$="A" THEN LET R0=10,R0$="S",R3$=""
1090 IF P7$="Z" THEN LET R0=P[2],R0$="Z",R3$=""
1095 GOTO 3900
1200 REM "LABELS/RECORD"
1220 LET V0$="Z",V1$="R",V2$="1",V3$="",V0=3,V1=40,V2=5
1230 LET V4$="Enter The Number of Labels Per Record"
1240 GOSUB 7000
1260 IF V<1 THEN GOTO 1200
1280 LET W3=V
1290 PRINT @(0,22),'CL',
1295 GOTO 3900
1400 REM "RESTART
1420 LET V0$="Y",V1$="CR",V2$="N",V4$="Y=Yes N=No",V0=1,V1=40,V2=7
1440 GOSUB 7000
1450 LET P9$=V$
1460 IF P9$="N" THEN LET P6$="",P8$=""; IF V3<>2 THEN GOTO 4000
1470 GOTO 3900
1600 REM "STARTING CODE
1620 LET V0$=R0$,V1$="KC",V2$=P6$,V3$=R3$,V0=R0,V1=40,V2=9
1630 LET V4$="Enter The Starting Vendor Code"
1640 GOSUB 7000
1660 LET P6$=V$
1680 IF P6$="" THEN PRINT @(V1,V2),"First"
1695 GOTO 3900
1700 REM "ENDING CODE"
1710 LET V0$=R0$,V1$="KC",V2$=P8$,V3$=R3$,V0=R0,V1=40,V2=11
1715 LET V4$="Enter The Ending Vendor Code"
1720 GOSUB 7000
1730 IF V3=4 THEN GOTO 9900
1740 IF V3=3 THEN CALL "SYC.LV",V$
1750 IF V3=2 THEN GOTO 1790
1760 IF V$<P6$ THEN GOTO 1700
1770 LET P8$=V$
1780 IF P8$="" THEN PRINT @(V1,V2),"Last"
1790 GOTO 3900
3900 REM "NEXT FIELD"
3910 IF V3<>2 THEN GOTO 3940
3920 IF I0>0 THEN LET I0=I0-1
3930 GOTO 3950
3940 LET I0=I0+1
3960 ON I0 GOTO 1000,1200,1400,1600,1700,4000
4000 REM "CORRECT?"
4010 LET V0$="Y",V1$="CE",V2$="",V3$="",V4$="Is The Above Information Correct (Y/N)? ",V0=1,V1=FNV(V4$),V2=22
4020 GOSUB 7000
4040 IF V3=4 THEN GOTO 9900
4050 IF V$="N" THEN GOTO 1000
4060 IF V$<>"Y" THEN GOTO 4000
4100 CALL "SYC.GA",7,1,"","",STATUS
4110 IF STATUS THEN GOTO 9900
4200 RUN "APR.KB"
7000 REM " --- Standard Input Routine (15May95)"
7010 CALL "SYC.IA",V0$,V1$,V2$,V3$,V4$,PGM(-2),V0,V1,V2,V$,V,V3,DISPLAY_LEN
7020 IF V3=999 THEN GOTO 7200
7030 IF V3=127 THEN GOTO 7100
7090 RETURN 
7100 REM " --- Escape During Input"
7110 CALL "SYC.ES",ERR=7200,PGM(-2),TCB(8),E$,E2,V3
7120 IF V3<>127 THEN GOTO 7000
7130 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
7200 REM " --- Error During Input"
7210 ESCAPE
7290 GOTO 7000
8000 REM "FUNCTIONS"
8080 DEF FNV(Q$)=42+INT(LEN(Q$)/2)
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9210 SETERR 9000
9220 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9800 REM " --- Display Parameter record error"
9810 LET LINE_ERR$=LST(PGM(TCB(5)))
9820 CALL "SYC.YA",LINE_ERR$,N2$,ERR
9900 RUN "SYS.AA"
9999 END
