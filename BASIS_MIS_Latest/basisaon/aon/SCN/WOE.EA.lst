0120 LET FILES$[4]="WOE-04",FILES$[5]="SYS-01",FILES$[6]="IVM-02"
0125 LET FILES$[7]="WOT-01",FILES$[8]="WOT-21",FILES$[9]="WOT-31"
0130 LET FILES$[10]="IVM-10",FILES$[11]="WOE-02",FILES$[12]="WOE-22"
0135 LET FILES$[13]="WOE-32",FILES$[14]="WOM-10"
0150 CALL "SYC.DA",1,1,14,FILES$[ALL],OPTIONS$[ALL],CHANNELS[ALL],BATCH,STATUS
0155 IF STATUS THEN GOTO 9900
0160 LET WOE01_DEV=CHANNELS[1],IVM01_DEV=CHANNELS[2],IVM03_DEV=CHANNELS[3]
0165 LET WOE04_DEV=CHANNELS[4],SYS01_DEV=CHANNELS[5],IVM02_DEV=CHANNELS[6]
0170 LET WOT01_DEV=CHANNELS[7],WOT21_DEV=CHANNELS[8],WOT31_DEV=CHANNELS[9]
0175 LET IVM10_DEV=CHANNELS[10],WOE02_DEV=CHANNELS[11],WOE22_DEV=CHANNELS[12]
0180 LET WOE32_DEV=CHANNELS[13],WOM10_DEV=CHANNELS[14]
0200 REM " --- IOLISTs"
0210 WOE01A: IOLIST A0$(1),A1$(1),A[ALL]
0220 IVM01A: IOLIST D0$,D1$(1),D2$
0230 WOS01A: IOLIST X$,P1$,P2$,P9$,P4$,M0$,M1$,M2$,M3$
0240 SYS01T: IOLIST X$,F0$,X$,X$,X$,F4$,F5$
0250 IVS01A: IOLIST X$,X$,P2$,P3$
0260 GLS01A: IOLIST X$,X$,G2$,G3$
0270 WOREQ: IOLIST W0$,W1$,W[ALL]
0280 WOE04B: IOLIST B0$
0290 WOE06A: IOLIST W0$,W1$,W[ALL]
0300 WOTRAN: IOLIST W0$,W1$,W[ALL]
0310 POE12A: IOLIST L0$,L1$,L2$,L3$,L4$,L5$,L6$,L[ALL]
0320 POE11A: IOLIST L0$,L1$,L2$,L3$,L4$,L5$,L6$,L[ALL]
0330 BMS01A: IOLIST *,*,M4$,M5$,M4,M5
0340 ARS01A: IOLIST *,*,X$,*,*,P8$
0350 APS01A: IOLIST *,*,X$,*,*,P7$
0360 PRS01A: IOLIST *,*,X$,*,*,P6$
0370 WOM10A: IOLIST Y0$
0375 WOM10A2: IOLIST X1$
