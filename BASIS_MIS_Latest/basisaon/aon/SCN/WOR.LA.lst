0960 PRINT @(COLUMN,11),SORTSEQ$(1,11)+SORTSEQ$(15)
0965 GOTO 0935
0970 IF WOADJ_DEV=WOE42_DEV THEN GOTO 0990
0975 LET WOADJ_DEV=WOE42_DEV
0980 GOTO 0930
0990 CALL "SYC.NB","Printing",18,COLUMN
1000 REM " --- Init Read"
1020 READ (WOW08_DEV,KEY=N0$,DOM=1100)
1100 REM " --- Main Read"
1120 LET K$=KEY(WOW08_DEV,END=4000)
1130 IF POS(N0$=K$)<>1 THEN GOTO 4000
1135 READ (WOW08_DEV,KEY=K$)
1140 IF K$(15,1)="O" THEN READ (WOE12_DEV,KEY=K$)IOL=WOE12A
1150 IF K$(15,1)="S" THEN READ (WOE42_DEV,KEY=K$)IOL=WOE42A
1180 PRINT @(COLUMN,11),A0$(5,7)," ",FNB$(A0$(12)),
1200 REM " --- W/O Header
1210 DIM D1$(138),C1$(138)
1220 FIND (WOE01_DEV,KEY=K$(1,11),DOM=1240)IOL=WOE01A
1230 IF D0$(14,1)<>"I" THEN GOTO 1260
1240 DIM DESC$(60)
1245 FIND (IVM01_DEV,KEY=N0$+D0$(56),DOM=1260)IOL=IVM01A
1250 LET D1$(1,30)=DESC$(1,MIN(DESC[1],30))
1260 LET C0$=D0$,C1$=D1$
1300 REM " --- Get Additional Data
1320 IF A0$(15,1)="O" THEN READ (WOT01_DEV,KEY=A0$,DOM=1100)IOL=WOT01A
1330 IF A0$(15,1)="S" THEN READ (WOT31_DEV,KEY=A0$,DOM=1100)IOL=WOT31A
1360 FIND (WOE01_DEV,KEY=N0$+"  "+A1$(1,7),DOM=1380)IOL=WOE01B
1370 IF POS(" "<>A1$(8,3))=0 THEN LET A1$(8,3)=A0$(12,3)
1380 IF NUM(A1$(1,7))=0 THEN LET A1$(1,7)=A0$(5,7)
1390 ON POS(A0$(15,1)="OS")-1 GOTO 1400,1500
1400 REM " --- Get Employee/Op Code
1410 DIM F0$(30),F1$(20)
1415 LET F0$(1)="*** Missing Employee ***",F1$(1)="Missing Op Code"
1420 FIND (EMPCODE_DEV,KEY=N0$+B1$(7,9),DOM=1430)IOL=EMPCODE
1430 LET D9$=F0$(1,16)+", "+F0$(17,14); GOSUB 5700
1440 LET F0$=D9$
1450 FIND (OPCODE_DEV,KEY=N0$+B1$(4,3),DOM=1470)IOL=OPCODE
1460 LET F1$=F1$(6,20)
1470 LET F1$=F1$(1,POS("   "=F1$+"   ")-1)
1480 LET F0$=F0$(1,POS("   "=F0$+"   ")-1)
1485 IF LEN(F0$)+37>124-M1-M2-M3*3-6 THEN LET F0$=F0$(1,124-M1-M2-M3*3-6-37)
1487 IF LEN(F1$)+37>124-M1-M2-M3*3-6 THEN LET F1$=F1$(1,124-M1-M2-M3*3-6-37)
1490 GOTO 2000
1500 REM " --- Get Vendor Name
1510 DIM F0$(30)
1515 LET F0$(1)="*** Missing Vendor ***"
1520 FIND (APM01_DEV,KEY=N0$+B1$(4,6),DOM=2000)IOL=APM01A
2000 REM " --- Print It Here
2020 IF L>L9 THEN GOSUB 5000
2040 PRINT (7)A0$(5,7),"  ",D0$(12,2),"  ",D0$(14,1),"  ",FNB$(A0$(12)),"  ",A0$(15,1),"  ",A0$(16,3),
2050 IF A0$(15,1)="O" THEN PRINT (7)@(124-M1-M2*3-M3*3),B[5]:M2$,B[3]:M3$,B[1]-B[3]:M3$,@(124-M1-M2*2),B[6]:M2$,
2060 IF A0$(15,1)="S" THEN PRINT (7)@(124-M1-M2*2-M3),B[1]:M3$,
2100 PRINT (7)@(124-M1-M2*3-M3*3-7),"Orig: ",@(124-M1-M2),B[0]:M2$,B[2]:M1$,
2120 IF A1$(1,7)<>A0$(5,7) THEN PRINT (7)@(124),A1$(1,7) ELSE PRINT (7)""
2200 REM " --- Adj Here
2220 PRINT (7)@(3),D1$(1,30),
2240 IF A0$(15,1)="O" THEN PRINT (7)@(124-M1-M2*3-M3*3),A[4]:M2$,A[1]:M3$,A[2]:M3$,@(124-M1-M2*2),A[3]:M2$,
2250 IF A0$(15,1)="S" THEN PRINT (7)@(124-M1-M2*2-M3),A[1]:M3$,
2260 PRINT (7)@(124-M1-M2*3-M3*3-6),"Adj:",@(124-M1-M2),A[0]:M2$,(A[0]+A[3])*(A[1]+A[2]):M1$,
2270 IF A1$(8,3)<>A0$(12,3) THEN PRINT (7)@(124),FNB$(A1$(8,3)) ELSE PRINT (7)""
2300 REM " --- Net
2310 IF A0$(15,1)="O" THEN PRINT (7)@(3),FNF$(B1$(7,C4),C5$),"  ",B1$(4,3)," ",F1$,@(124-M1-M2*3-M3*3),A[4]-B[5]:M2$,A[1]-B[3]:M3$,A[2]-(B[1]-B[3]):M3$,@(124-M1-M2*2),A[3]-B[6]:M2$,
2320 IF A0$(15,1)="S" THEN PRINT (7)@(3),FNF$(B1$(4,C2),C3$),"   ",B1$(10,10),@(124-M1-M2*2-M3),A[1]-B[1]:M3$,
2330 PRINT (7)@(124-M1-M2*3-M3*3-6),"Net:",@(124-M1-M2),A[0]-B[0]:M2$,(A[0]+A[3])*(A[1]+A[2])-B[2]:M1$,
2340 IF B[2]<>(A[0]+A[3])*(A[1]+A[2]) THEN PRINT (7)@(124),"***" ELSE PRINT (7)""
2350 PRINT (7)@(3),F0$
2380 PRINT (7)""
2390 LET L=L+5,T0=T0+(A[0]+A[3])*(A[1]+A[2])-B[2]
2400 REM " --- Check G/L Posting Dates"
2410 IF GL$<>"" THEN GOTO 2500
2420 LET GLSTATUS=0
2430 CALL "GLC.EA",A0$(12,3),"N",PERIOD$,YEAR$,GLSTATUS
2440 IF GLSTATUS>0 THEN CALL "GLC.GA",7,A0$(12,3),GLSTATUS,L,ERRORS,STATUS
2450 IF FNP$(A1$(8,3))="" THEN GOTO 2500
2460 CALL "GLC.EA",A1$(8,3),"N",PERIOD$,YEAR$,GLSTATUS
2470 IF GLSTATUS>0 THEN CALL "GLC.GA",7,A1$(8,3),GLSTATUS,L,ERRORS,STATUS
2500 REM " --- Post Original Dates
2510 LET W0$=N0$+A0$(12,3)+D0$(12,2)
2515 DIM W[3]
2520 FIND (WOW06_DEV,KEY=W0$,DOM=2540)IOL=WOW06A
2540 IF A0$(15,1)="O" THEN LET W[0]=W[0]-B[3]*(B[0]+B[6]),W[1]=W[1]-(B[2]-B[3]*(B[0]+B[6]))
2550 IF A0$(15,1)="S" THEN LET W[2]=W[2]-B[2]
2560 LET W[3]=W[3]-B[2]
2580 IF D0$(12,2)=C0$(12,2) AND A0$(12,3)=A1$(8,3) THEN GOTO 2650
2590 WRITE (WOW06_DEV,KEY=W0$)IOL=WOW06A
