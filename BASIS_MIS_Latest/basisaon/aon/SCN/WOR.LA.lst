2630 DIM W[3]
2640 FIND (WOW06_DEV,KEY=W0$,DOM=2650)IOL=WOW06A
2650 IF A0$(15,1)="O" THEN LET W[0]=W[0]+A[1]*(A[0]+A[3]),W[1]=W[1]+A[2]*(A[0]+A[3])
2660 IF A0$(15,1)="S" THEN LET W[2]=W[2]+A[1]*A[0]
2670 LET W[3]=W[3]+(A[1]+A[2])*(A[0]+A[3])
2680 WRITE (WOW06_DEV,KEY=W0$)IOL=WOW06A
3000 REM " --- Generate G/L Summary"
3010 IF GL$<>"Y" THEN GOTO 3900
3015 DIM Y0$(70,"0"),Z0$(70,"0")
3020 FIND (WOM10_DEV,KEY=N0$+"A"+D0$(12,2))IOL=WOM10A
3030 LET Z0$=Y0$
3040 IF C0$(12,2)=D0$(12,2) THEN GOTO 3100
3050 FIND (WOM10_DEV,KEY=N0$+"A"+C0$(12,2),DOM=1460)IOL=WOM10A2
3100 REM " --- Back Out The Distribution
3130 LET ACCOUNT$=Y0$(1,10),GLDATE$=A0$(12,3),AMOUNT=-B[2],UNITS=0
3140 LET REF1$="WO "+A0$(5,7),REF2$="Seq "+A0$(16,3),REF3$=FNB$(A1$(8,3))
3150 LET MEMO$=CM$
3160 GOSUB GLRECAP
3190 ON POS(A0$(15,1)="OS")-1 GOTO 3200,3300,3900
3200 REM " --- Labor "    
3220 LET ACCOUNT$=Y0$(21,10),AMOUNT=(B[0]+B[6])*B[3],MEMO$=LM$
3240 GOSUB GLRECAP
3260 LET ACCOUNT$=Y0$(31,10),AMOUNT=(B[0]+B[6])*B[4]
3280 GOSUB GLRECAP
3290 GOTO 3500
3300 REM " --- Subcontracts "     
3320 LET ACCOUNT$=Y0$(41,10),AMOUNT=B[2],MEMO$=SM$
3340 GOSUB GLRECAP
3500 REM " --- Add It Back In With New Information "
3520 LET ACCOUNT$=Z0$(1,10),GLDATE$=A1$(8,3)
3525 LET AMOUNT=ROUND((A[0]+A[3])*A[1],2)+ROUND((A[0]+A[3])*A[2],2)
3530 LET REF1$="WO "+C0$(5,7),REF2$="Seq "+A0$(16,3),REF3$=FNB$(A0$(12,3))
3540 LET MEMO$=CM$
3550 GOSUB GLRECAP
3590 ON POS(A0$(15,1)="OS")-1 GOTO 3600,3700
3600 REM " --- Labor "    
3610 LET ACCOUNT$=Z0$(21,10),AMOUNT=((-A[0])-A[3])*A[1],MEMO$=LM$
3620 GOSUB GLRECAP
3650 LET ACCOUNT$=Z0$(31,10),AMOUNT=((-A[0])-A[3])*A[2]
3660 GOSUB GLRECAP
3690 GOTO 3900
3700 REM " --- Subcontracts "    
