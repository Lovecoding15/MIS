0665 CALL "SYC.DA",1,11,15,FILES$[ALL],OPTIONS$[ALL],CHANNELS[ALL],BATCH,STATUS
0666 IF STATUS THEN GOTO 9900
0670 LET OPCODE_DEV=CHANNELS[11],EMPCODE_DEV=CHANNELS[12],APM01_DEV=CHANNELS[13]
0675 IF STATUS THEN GOTO 9900
0680 IF CHANNELS[14] THEN LET GLM01_DEV=CHANNELS[14],GLW11_DEV=CHANNELS[15]
