0430 FIND (SYS01_DEV,KEY=N0$+"IV00",DOM=9800)IOL=IVS01A
0435 LET I9=NUM(I2$(1,2)),I1=NUM(I2$(5,1)),I2=NUM(I2$(7,2))
0440 IF AR$="N" THEN GOTO 0455
0445 FIND (SYS01_DEV,KEY=N0$+"AR00",DOM=9800)IOL=ARS01A
0450 LET C0=NUM(C0$(1,2)),C1=LEN(C1$)
0455 IF AP$="N" THEN GOTO 0470
0460 FIND (SYS01_DEV,KEY=N0$+"AP00",DOM=9800)IOL=APS01A
0465 LET C2=NUM(C2$(1,2)),C3=LEN(C3$)
0470 IF PR$="N" THEN LET C4=NUM(P2$(1,2)),C5$=M0$; GOTO 0485
0475 FIND (SYS01_DEV,KEY=N0$+"PR00",DOM=9800)IOL=PRS01A
0480 LET C4=NUM(C4$(1,2)),C5=LEN(C5$)
0485 LET GL$="N",STATUS=0,SOURCE$=PGM(-2)
0490 CALL "GLC.BA",ERR=0495,SOURCE$,N2$,GLW11$,GL$,STATUS
0495 IF STATUS THEN GOTO 9900
