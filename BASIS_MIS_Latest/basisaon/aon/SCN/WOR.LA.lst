3720 GOSUB GLRECAP
3900 REM " --- Loop back for next record"
3990 GOTO 1100
4000 REM " --- End"
4020 IF L+2>L9 THEN GOSUB 5000
4040 PRINT (7)@(124-M1),J$(1,M1-1),'LF',@(37),"Report Total: ",@(124-M1),T0:M1$
4100 REM " --- Totals By Type
4120 LET L=L9+5
4170 DIM T[3],U[3]
