0600 REM " --- Additional File Opens"
0630 IF BM$="Y" THEN LET FILES$[11]="BMM-08"
0635 IF BM$<>"Y" THEN LET FILES$[11]="WOM-02"
0640 IF PR$="Y" THEN LET FILES$[12]="PRM-01"
0645 IF PR$<>"Y" THEN LET FILES$[12]="WOM-01"
0650 IF AP$="Y" THEN LET FILES$[13]="APM-01"
