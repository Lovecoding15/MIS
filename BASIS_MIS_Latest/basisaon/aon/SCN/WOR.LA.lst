0170 LET WOM10_DEV=CHANNELS[7],IVM01_DEV=CHANNELS[8],WOW08_DEV=CHANNELS[9]
0175 LET WOW06_DEV=CHANNELS[10]
0200 REM " --- IOLISTs"
0210 WOE12A: IOLIST A0$,A1$,A[ALL]
0220 WOE42A: IOLIST A0$,A1$,A[ALL]
0230 WOT01A: IOLIST B0$,B1$,B[ALL]
