4200 READ (WOW06_DEV,END=4400)IOL=WOW06A
4210 IF POS(N0$=W0$)<>1 THEN GOTO 4400
4220 IF W0$(3,3)<>T0$ THEN GOSUB 6000
4230 DIM X0$(39),X1$(110)
4240 FIND (WOM10_DEV,KEY=N0$+"A"+W0$(6),DOM=4260)IOL=WOM10A3
4260 IF L>L9 THEN GOSUB 5200
4280 PRINT (7)W0$(6),"  ",X0$(6,20),@(30),W[0]:M1$,W[1]:M1$,W[2]:M1$,W[3]:M1$
4300 FOR X=0 TO 3; LET T[X]=T[X]+W[X],U[X]=U[X]+W[X]; NEXT X
4310 LET L0=L0+1
4320 GOTO 4200
4400 IF L+2>L9 THEN GOSUB 5000
4410 LET T9$="E"
4415 GOSUB 6000
4420 IF L>L9 THEN GOSUB 5200
4440 PRINT (7)@(30),J$(1,M1*4)
4460 PRINT (7)@(3),"Report Totals: ",@(30),U[0]:M1$,U[1]:M1$,U[2]:M1$,U[3]:M1$
4900 REM 
4910 LET UPDATE$="WOU.GA",PAGE=H,WIDTH=H0,F0$=TERMF0$
4980 IF GL$="Y" THEN RUN "GLR.XA"
4990 RUN UPDATE$
5000 REM " --- Heading"
5010 LET L=0
5020 CALL "SYC.HA",7,HEADINGS$[ALL],HEADINGS,H,H0,WHEN$,CLOCK$,STATUS
5030 IF STATUS>0 THEN EXITTO 9900
5040 PRINT (7)""
5045 IF O9=1 THEN GOTO 5190
5060 PRINT (7)@(18),"Transaction",@(124-M1-M2*2-M3*3-5),"Qty",@(124-M1-M2*2-M3*2-7),"Direct",@(124-M1-M2*2-M3-9),"Overhead",@(124-M1-M2*2-5),"Unit",@(124-M1-M2-5),"Setup",@(124),"New WO #"
5070 PRINT (7)"WO #",@(7),"Type Cat",@(18),"Date",@(25),"Src Seq",@(124-M1-M2*2-M3*3-6),"Cmplt",@(124-M1-M2*2-M3*2-6),"Labor",@(124-M1-M2*2-M3-6),"Labor",@(124-M1-M2*2-5),"Cost",@(124-M1-M2-5),"Units",@(124-M1-6),"Units",@(124-10),"Extension",@(124),"Adj Date"
5080 PRINT (7)@(3),
5090 IF P3$(3,1)="Y" THEN PRINT (7)@(3),"Vendor/PO #/",
5100 PRINT (7)"Employee/Op Code",'LF'
5190 RETURN 
5200 REM " --- Dates Here
5210 LET O9=1
5220 IF L>L9 THEN GOSUB 5000
5230 PRINT (7)"*** Work Order Adjustment Summary By Type ***",'LF'
5240 PRINT (7)@(30+M1-7),"Direct",@(30+M1*2-9),"Overhead",@(30+M1*3-5),"Subs"
5250 PRINT (7)@(30+M1-6),"Labor",@(30+M1*2-6),"Labor",@(30+M1*3-7),"Amount",@(30+M1*4-6),"Total",'LF'
5290 RETURN 
5300 REM " --- Date Subheading"
5320 IF L>L9 THEN GOSUB 5200
5340 PRINT (7)"Transaction Date: ",FNB$(T0$),":",'LF'
5360 LET L=L+2
5390 RETURN 
5700 REM " --- Compress Desc
5730 LET Z=POS(" "<>D9$); IF Z>1 THEN LET D9$=D9$(Z)
