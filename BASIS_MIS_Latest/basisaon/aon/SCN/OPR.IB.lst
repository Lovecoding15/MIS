5220 PRINT (7)'LF',@(0),FNF$(A0$(5,P[0]),M0$),@(M0+1),B1$(1,WIDTH),@(39-LEN(STAT$)),STAT$,@(40),A0$(11,7),@(48),FNB$(A0$(77,3)),@(57),FNB$(A0$(27,3)),@(66),A0$(96,3),@(70),A0$(59,3),@(74),A0$(49,10),@(85),A0$(32,10),@(98),A0$(66,2),@(103),W$,@(120),A0$(80,2),@(127),A0$(68,1)
5221 IF A0$(69,1)<>"N" THEN PRINT (7)@(40),"**Locked Flag="+A0$(69,1)+"**",
5225 PRINT (7)""
