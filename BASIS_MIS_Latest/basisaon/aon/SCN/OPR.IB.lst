1320 READ (ARE03_DEV,KEY=K1$(1,17)+$FF$,DOM=1100)
1400 REM " --- Heading "
1405 GOSUB 6000
1410 IF A0$(21,1)<>"V" THEN GOTO 1900
1420 REM " --- Void "
1430 IF L+2>L9 THEN GOSUB 5000
1440 PRINT (7)'LF',@(0),FNF$(A0$(5,P[0]),M0$)," ",B1$(1,30)," ",A0$(11,7)," ",A0$(42,7)," ",FNB$(A0$(24,3))," ",A0$(96,3)," ",A0$(59,3)," *** VOID ***"
