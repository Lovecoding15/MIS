4990 GOTO 9900
5000 REM "HEADING"
5010 LET L=HEADINGS
5020 CALL "SYC.HA",7,HEADINGS$[ALL],HEADINGS,H,H0,WHEN$,CLOCK$,STATUS
5030 IF STATUS>0 THEN EXITTO 9900
5100 PRINT (7)"-- Customer --",@(41),"Order",@(50),"Order",@(59),"Ship",@(75),"P.O.",@(97),"Dist",@(120),"Tax"
5110 PRINT (7)"Number",@(8),"Name",@(40),"Number",@(50),"Date",@(59),"Date",@(66),"By",@(70),"Sls",@(74),"Number",@(85),"Ship Via",@(97),"Code",@(103),"Terms",@(120),"Code",@(126),"Prntd?"
5180 LET L=L+4
