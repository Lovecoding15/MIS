0880 FOR X=0 TO 14
0885 LET H[X]=O[X]
0890 NEXT X
0900 LET H[5]=H[5]+M2-6,H[6]=H[6]+M2-6,H[7]=H[7]+M2-5,H[8]=H[8]+M9-4
