0685 IF M2<8 THEN CALL "SYC.RM",M2$,M2,8,12
0690 IF M3<8 THEN CALL "SYC.RM",M3$,M3,8,12
0695 LET XM1$=M1$,XM2$=M2$,XM3$=M3$
0700 REM " --- Background"
