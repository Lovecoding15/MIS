5235 PRINT (7)@(H[0]),"Ln Cd",@(H[2]),"Prd Whs Item",@(H[5]),"Order",@(H[6]),"Order",@(H[7]),"Ship",@(H[8]),"Pct",@(H[9]),"Price",@(H[10])," Price",@(H[11]),"Cost",@(H[12]),"  Cost",@(H[13]),"Profit",@(H[14]),"GP %"
