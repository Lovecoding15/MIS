0635 LET H$(1)=H4$,H$(FNC(H8$,H0))=H8$,H$(X)=H6$,HEADINGS$[3]=H$
0640 LET H$(1)=H5$,H$(FNC(H10$,H0))=H10$,H$(X)=H7$,HEADINGS$[4]=H$
0645 LET HEADINGS$[5]=H9$,XM1=M1,XM2=M2,XM3=M3
