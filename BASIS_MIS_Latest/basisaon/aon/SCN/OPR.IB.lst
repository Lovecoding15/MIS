5820 PRINT (7)@(O[8]),W[8]:M9$,@(O[9]),W[1]:M4$,@(O[10]),PRICE:M1$,@(O[11]),W[0]:M3$,@(O[12]),COST:M1$,@(O[13]),PRICE-COST:M1$,@(O[14]),FND(PRICE-COST,PRICE):M9$,
5840 IF X0$(25,1)="O" THEN GOTO 5900
