0910 LET H[12]=H[12]+M1-9,H[13]=H[13]+M1-7,H[14]=H[14]+M9-4
1000 REM " ---Initial File Read "
1020 READ (ARE03_DEV,KEY=N0$+"  "+S0$,DOM=1030)
1100 REM " --- Main Read "
1120 LET K1$=KEY(ARE03_DEV,END=4000)
