0030 REM "+-----------------------------------------------------------+"
0031 REM "MAS * DCF * WA# 208
0040 REM 
0070 SETERR 9000
0080 SETESC 9000
0090 PRECISION P[1]
0500 REM " --- Initializations"
0510 DIM DATEP[8]
0550 LET SERIAL$=A0$,O0$="SNM.MA",INCLUDE$="Y"
0555 LET DATEP[1]=29,DATEP[2]=32,DATEP[3]=35,DATEP[4]=38
0560 LET DATEP[5]=44,DATEP[6]=47,DATEP[7]=41,DATEP[8]=50
0570 LET BEGSER$=A0$,ENDSER$=A0$
0575 LET H1$=F5$+" Listing"
0800 REM " --- Options"
0810 CALL "SYC.YN",0,"Include Serial Number Comments",4,INCLUDE$,V3
0820 ON V3 GOTO 0830,0800,0800,0800,4900
0830 ON POS(INCLUDE$="YN") GOTO 0800,4000,4000
4000 REM " --- Run report overlay"
4090 RUN "SNR.RA"
4900 REM " --- Run menu overlay"
4990 RUN "SNM.MA"
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- <F4>/END Option For Locked Record"
9220 IF ERR=0 THEN RUN "IVM.MA"
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9900 RUN "SYS.AA"
9999 END
