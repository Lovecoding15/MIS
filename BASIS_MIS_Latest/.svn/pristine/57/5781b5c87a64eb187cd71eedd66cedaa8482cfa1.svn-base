1200 REM "--- Define MAS Parameter References to PRM$[MODULE,ELEMENT]
1210 REM "    Where ELEMENT = "
1220 LET INSTALL=0; REM "Installation Flag For Application <Y/N>"
1240 LET KEYLEN=1; REM " Parameterized Length Of Key, ie: Cust Number Length"
1260 LET KEYMAX=2; REM " Maximum Length Of Key, ie: Cust=6, Item=20
1280 LET KEYMASK=3; REM "Customer, Vendor, Employee Number Mask...."
1300 LET CPERIOD=4; REM "Current Period"
1310 LET CYEAR=5; REM "  Current Year"
1320 LET DMASK=6; REM "  Dollar Mask"
1330 LET UMASK=7; REM "  Unit Mask"
1340 LET RMASK=8; REM "  Rate Mask"
1350 LET TWO=9; REM "    ADD+ON 2$ Parameter String"
1360 LET THREE=10; REM " ADD+ON 3$ Parameter String"
1370 LET FOUR=11; REM "  ADD+ON 4$ Parameter String"
1380 LET EMASK=12
2000 REM "--- Define MAS Parameter References to PRM$[TERMINAL,ATTRIBUTES]
2020 LET TOPCODE=1; REM "Current Operator Code"
2040 LET TDATE=2; REM "Terminal Date"
2060 LET TMENU=3; REM "Terminal Menu"
2080 LET TPRINTER=4; REM "Printer Selected"
2100 LET TSECURE=5; REM "Security Level"
