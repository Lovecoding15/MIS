2180 LET TPROG=8; REM "Terminal Program"
2200 LET TPOPTS=9; REM "Terminal Printer Options"
2220 LET TFNAME=10; REM "Terminal Firm Name"
2240 LET TPNAME=11; REM "Terminal Program Name"
2250 LET TLDATE=12; REM "Long (YYMMDD) Date"
4090 LET O0=9
4190 GOTO 9900
9000 REM "---- Error Exit"
