6000 REM " --- Update sample manifest entry files"
6010 DIM A0$(20),A1$(168),A2$(64),A[8],W0$(20),W1$(128),W[20]
6020 LET NUMBER=NUMBER+1,STATE$=ADDR3$(LEN(ADDR3$)-1)
6100 REM " --- Initialize header record"
6110 LET A0$(1)=N0$+"M"+STR(NUMBER:M0$),A0$(18,3)="000"
6120 LET A1$(1,6)=M0$,A1$(7,24)=NAME$,A1$(31,24)=ADDR1$,A1$(55,24)=ADDR2$
6130 LET A1$(79,24)=ADDR3$,A1$(127,5)=ZIPCODE$,A1$(142,2)=STATE$
6200 REM " --- Initialize detail record"
6210 DIM G0$(10),G1$(128)
6220 LET G0$(1)=N0$+"G"+SERVICE$
6230 FIND (SHM10_DEV,KEY=G0$,DOM=6280)IOL=SHM10G
6240 LET G1$=FNP$(G1$),L=LEN(G1$)
6250 IF L=0 THEN GOTO 6280
6260 FOR X=1 TO L
6265 IF G1$(X,1)<>" " THEN IF W1$(X,1)=" " THEN LET W1$(X,1)=G1$(X,1)
6270 NEXT X
6280 LET W0$(1)=A0$,W0$(18,3)="001",W1$(4,7)=SERVICE$,W1$(11,1)=P9$
6290 LET W1$(59,2)=P8$,W[0]=WEIGHT,W[1]=VALUE
6300 REM " --- Generate 10-digit Package Tracking Number"
6310 LET TRACKING=TRACKING+1,V=7-MOD(TRACKING,7)
6320 LET W1$(108,18)=STR(TRACKING:FILL(9,"0"))+STR(V:"0")
6400 REM " --- Other detail fields"
6410 LET W1$(92,1)="N",W1$(93,1)="N"
6420 IF A1$(142,2)="AK" THEN LET W1$(92,1)="Y"
6430 IF A1$(142,2)="HI" THEN LET W1$(93,1)="Y"
6500 REM " --- Write records"
6510 PRINT @(COLUMN,11),W0$(3,7),"-",W0$(18,3),
6520 WRITE (SHE01_DEV,KEY=A0$)IOL=SHE01A
6530 WRITE (SHE02_DEV,KEY=W0$)IOL=SHE02A
6590 RETURN 
