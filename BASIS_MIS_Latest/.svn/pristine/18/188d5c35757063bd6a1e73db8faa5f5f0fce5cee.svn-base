0010 REM "DDX - Export ADD+ON Data Dictionary"
0020 REM "Program DDX.XA"
0022 REM "+-----------------------------------------------------------+"
0024 REM "| ADD+ON Software, Inc. Advantage/V Version 6.0.0 - 15May96 |"
0026 REM "|      Copyright (c) 1996 Open Systems Holdings Corp.       |"
0028 REM "|                  All Rights Reserved                      |"
0030 REM "+-----------------------------------------------------------+"
0040 REM 
0070 BEGIN 
0080 SETERR 9000
0090 SETESC 9000
0400 REM " --- Parameters"
0410 DIM INFO$[20]
0420 CALL "SYC.VA","GE",INFO$[ALL]
0430 LET GE$=INFO$[20]
0500 REM " --- Initializations"
0550 LET SELECTION$="DDX.EA"
1000 REM " --- GENERAL Report Writer installed?"
1010 IF GE$<>"Y" THEN GOTO 4000
2000 REM " --- Process submenu"
2010 DIM MENUS$[1]
2020 LET MENUS=1,MENUS$[1]="DDX.XA",WIN_X=0,WIN_Y=4,O0=1
2030 CALL "SYC.OA",MENUS,MENUS$[ALL],WIN_X,WIN_Y,DESCRIPTION$,SELECTION$
2040 IF SELECTION$="" THEN GOTO 9900
2050 PRINT @(0,3),'CE',
4000 REM " --- Run 1st export overlay"
4090 RUN SELECTION$,ERR=9900
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9800 REM " --- Display Parameter record error"
9810 LET LINE_ERR$=LST(PGM(TCB(5)))
9820 CALL "SYC.YA",LINE_ERR$,N2$,ERR
9900 RUN "SYS.AA"
9999 END
