0830 IF V$<>"YES" THEN GOTO 9900
0900 REM 
0910 DIM A[11],B[10],C[16],W[11]
0920 CALL "SYC.NB","Updating",7,COLUMN
0940 REM " --- Disallow 'M'enu option in Error Routine
0950 LET EXIT_CTRL=1
1000 REM " --- Initial Read"
1020 READ (WOE04_DEV,KEY=N0$+"  "+"B",DOM=1100)
1100 LET K0$=KEY(WOE04_DEV,END=4000)
1120 IF K0$(1,5)<>N0$+"  "+"B" THEN GOTO 4000
1140 EXTRACT (WOE01_DEV,KEY=K0$(1,4)+K0$(6,7),DOM=3800)IOL=WOE01A
1150 IF A0$(14,1)="R" THEN READ (WOE04_DEV); GOTO 1100
1160 PRINT @(COLUMN,11),A0$(5,7),
1170 LET REFS$[1]=A0$(34,6),REFS$[2]=A0$(40,7),REFS$[3]=A0$(5,7)
1180 LET REFS$[4]=A0$(12,2)
1200 REM " --- Calc Actuals From Transactions "
1210 DIM D[4],T[5]
1215 LET WOTRAN_DEV=WOT01_DEV
1220 READ (WOTRAN_DEV,KEY=A0$(1,11),DOM=1230)
1230 LET K$=KEY(WOTRAN_DEV,END=1295)
1240 IF K$(1,11)<>A0$(1,11) THEN GOTO 1295
1250 READ (WOTRAN_DEV,KEY=K$)IOL=WOTRAN
1260 LET X=POS(W0$(15,1)="MS")+2
1265 IF X>2 THEN LET D[X]=D[X]+W[2]; GOTO 1280
1270 LET D[1]=D[1]+W[0]*W[3],D[2]=D[2]+W[2]-W[0]*W[3]; REM "Split Direct & OH
1280 LET D[0]=D[0]+W[2]
1290 GOTO 1230
1295 IF WOTRAN_DEV=WOT01_DEV THEN LET WOTRAN_DEV=WOT21_DEV; GOTO 1220
1297 IF WOTRAN_DEV=WOT21_DEV THEN LET WOTRAN_DEV=WOT31_DEV; GOTO 1220
1300 LET T[0]=D[0]-A[3],T[0]=-T[0],T[1]=A[4]*A[5]; REM "CALC TOTALS
1370 DIM Y0$(40),Y1$(70,"0")
1380 FIND (WOM10_DEV,KEY=N0$+"A"+A0$(12,2),DOM=1390)IOL=WOM10A
1390 IF A0$(14,1)<>"I" THEN GOTO 2000
1400 REM " --- Determine GL Posting Accounts "                  
1405 READ (IVM01_DEV,KEY=N0$+A0$(56,20),DOM=1500)IOL=IVM01A
1410 IF R9$<>"Y" THEN LET Y2$=B6$(1,10)
1420 READ (IVM02_DEV,KEY=N0$+A0$(54,22),DOM=1500)IOL=IVM02A
1430 IF R9$="Y" THEN GOSUB 6600
1440 LET Y1$(11,10)=Y2$
1500 REM " --- Update Item ON ORDER Quantity "
1502 LET ACTION$="OO",ITEMS$[0]=N0$,ITEMS$[1]=A0$(54,2),ITEMS$[2]=A0$(56,20)
1503 LET REFS$[0]=A1$(91,3),REFS$[3]=A0$(5,7),ITEMS$[3]=""
1520 LET CLOSE_QTY=A[4],SCHEDULED_WO_TOTAL=A[0],CLOSED_PRIOR=A[2]
1530 LET COMPLETE_QTY=CLOSE_QTY+(SCHEDULED_WO_TOTAL-(CLOSED_PRIOR+CLOSE_QTY))
1540 IF A1$(94,1)="Y" THEN LET REFS[0]=-(COMPLETE_QTY)
1545 IF A1$(94,1)="N" THEN LET REFS[0]=-(CLOSE_QTY)
1550 GOSUB UPDATE_INVENTORY
1560 REM " --- Update Item ISSUED Quantity "
1570 LET ACTION$="WO"
1580 LET REFS$[0]=A1$(91,3),ITEMS$[3]=""
1590 IF A1$(96,1)="Y" THEN GOTO 2000
1600 LET REFS[0]=A[4],REFS[1]=A[5]
1610 GOSUB UPDATE_INVENTORY
2000 REM " --- Serial/Lotted "
2010 IF A1$(96,1)<>"Y" THEN GOTO 2400
2020 DIM X[2],W[11]
2030 READ (WOE06_DEV,KEY=A0$(1,11),DOM=2040)
2040 LET K1$=KEY(WOE06_DEV,END=2400)
2050 IF POS(A0$(1,11)=K1$)<>1 THEN GOTO 2400
2060 READ (WOE06_DEV,KEY=K1$)IOL=WOE06A
2070 IF W[4]<>0 THEN GOTO 2120
2080 IF A1$(94,1)<>"Y" THEN GOTO 2040
2090 IF W1$(1,1)="Y" THEN GOTO 2040
2100 REMOVE (WOE06_DEV,KEY=K1$); REM "Remove unclosed lots if complete
2110 GOTO 2040
2115 REM " --- Update Lot ISSUED Quantity "
2120 LET REFS[0]=W[4],REFS[1]=W[5],ITEMS$[3]=W1$(37,20)
2130 GOSUB UPDATE_INVENTORY
2300 REM " --- History Here"
2380 IF W1$(25,1)="Y" THEN LET W1$(1,1)="Y",W1$(2,3)=A1$(91,3)
2385 LET W[2]=W[2]+W[4],W[3]=W[3]+W[4]*W[5],W[4]=0,W[5]=0
2390 WRITE (WOE06_DEV,KEY=W0$(1,14))IOL=WOE06A
2395 GOTO 2040
2400 REM " --- Do Variances Here
2420 IF A1$(94,1)<>"Y" THEN LET T[0]=-T[1]; GOTO 2900
2430 IF Y0$(27,1)="A" THEN LET T[1]=D[0]-A[3]; GOTO 2900
2500 REM " --- Calc Standards"
2510 DIM E[4]
2515 LET WOREQ_DEV=WOE02_DEV
2520 READ (WOREQ_DEV,KEY=A0$(1,11),DOM=2530)
2530 LET K$=KEY(WOREQ_DEV,END=2595)
2540 IF K$(1,11)<>A0$(1,11) THEN GOTO 2595
2550 READ (WOREQ_DEV,KEY=K$)IOL=WOREQA
2560 LET X=POS(W0$(12,1)="BC")+2; IF X>2 THEN LET E[X]=E[X]+W[3]; GOTO 2580
2570 LET E[1]=E[1]+W[2]*W[6],E[2]=E[2]+W[3]-W[2]*W[6]; REM "Split Direct & OH
2580 LET E[0]=E[0]+W[3]
2590 GOTO 2530
2595 IF WOREQ_DEV=WOE02_DEV THEN LET WOREQ_DEV=WOE22_DEV; GOTO 2520
2596 IF WOREQ_DEV=WOE22_DEV THEN LET WOREQ_DEV=WOE32_DEV; GOTO 2520
2600 REM " --- Calc Variances
2620 PRECISION 2
2625 IF E[0]=0 THEN LET Y0=0; GOTO 2800
2630 IF A0$(14,1)<>"I" AND (A[0]=A[2]+A[4] OR E[0]=0 OR A1$(95,1)="N") THEN LET Y0=A[4]*A[5]+A[3]; GOTO 2800
2700 REM " --- Pro-Rate Standards"
2710 IF A0$(14,1)<>"I" THEN IF A[0]<>0 THEN LET Y0=E[0]*(A[2]+A[4])/A[0] ELSE LET Y0=0 ELSE LET Y0=A[4]*A[5]+A[3]
2720 IF Y0=E[0] THEN GOTO 2800
2725 IF E[0]=0 THEN LET E[1]=0,E[3]=0,E[4]=0; GOTO 2760
2730 LET E[1]=E[1]*Y0/E[0]
2740 LET E[3]=E[3]*Y0/E[0]
2750 LET E[4]=E[4]*Y0/E[0]
2760 LET E[2]=Y0-(E[1]+E[4]+E[3])
2800 REM " --- Calc Variance"
2820 LET T[2]=D[1]-E[1],T[4]=D[3]-E[3],T[5]=D[4]-E[4]
2830 LET T[3]=D[0]-(Y0+T[2]+T[4]+T[5]); REM "Any left overs goto OH VNCE
2900 REM " --- Post G/L Here"
2910 PRECISION 2
2915 LET Y1$=Y1$(1,20)+Y1$(51,40)
2920 FOR X=0 TO 5
2930 IF T[X]=0 THEN GOTO 2990
2940 LET ACCOUNT$=Y1$(X*10+1,10),WHEN$=A1$(91,3),AMOUNT=T[X],UNITS=0
2945 IF A0$(14,1)<>"I" THEN LET MEMO$=A1$(1,30) ELSE LET MEMO$=DESC$
2950 LET REF1$="WO "+A0$(5,7),REF2$="Whse "+A0$(54,2)
2955 LET REF3$="Type "+A0$(12,2)
2960 GOSUB GLPOST
2990 NEXT X
3000 REM " --- Complete It"
3010 IF A1$(94,1)="Y" THEN GOTO 3400
3020 IF A0$(31,3)<A1$(91,3) THEN LET A0$(31,3)=A1$(91,3)
3100 REM " --- Post To Customer Order Line Item (Partial) "
3110 IF A0$(14,1)<>"N" OR P9$(2,1)<>"Y" THEN GOTO 3180
3120 DIM W[14]
3130 PRECISION IVS01_PRECISION
3140 EXTRACT (ARE13_DEV,KEY=N0$+"  "+A0$(34,16),DOM=3180)IOL=ARE13A
3150 LET W[0]=A[5],W[4]=A[4]
3160 WRITE (ARE13_DEV,KEY=W0$(1,20))IOL=ARE13A
3170 PRECISION 2
3180 LET A[2]=A[2]+A[4],A[3]=A[3]+A[4]*A[5],A1$(91,5)="",A[4]=0,A[5]=0
3190 GOTO 3780
3400 REM " --- Complete, Complete"
3410 GOSUB 6700
3420 LET A[2]=A[2]+A[4],A[3]=A[3]+A[4]*A[5]
3430 LET A0$(15,1)="C",A0$(31,3)=A1$(91,3)
3440 REM " --- Post To Customer Order (Complete) "
3450 IF A0$(14,1)<>"N" OR P9$(2,1)<>"Y" THEN GOTO 3520
3460 DIM W[14]
3470 PRECISION IVS01_PRECISION
3480 EXTRACT (ARE13_DEV,KEY=N0$+"  "+A0$(34,16),DOM=3520)IOL=ARE13A
3490 LET W[0]=A[5],W[4]=A[4]
3500 WRITE (ARE13_DEV,KEY=W0$(1,20))IOL=ARE13A
3510 PRECISION 2
3520 LET A[4]=0,A[5]=0
3600 REM " --- Reset IVM-02 Committed If All Is Not Issued "
3610 REMOVE (WOE13_DEV,KEY=A0$(1,11)+"000",DOM=3620)
3620 READ (WOE23_DEV,KEY=A0$(1,11),DOM=3630)
3630 LET K$=KEY(WOE23_DEV,END=3720)
3640 IF K$(1,11)<>A0$(1,11) THEN GOTO 3720
3650 READ (WOE23_DEV,KEY=K$)IOL=WOE23A
3660 IF W[0]=W[1] THEN GOTO 3700
3670 LET ACTION$="UC",ITEMS$[0]=N0$,ITEMS$[1]=W1$(19,2),ITEMS$[2]=W1$(21,20)
3680 LET REFS[0]=W[0]-W[1],ITEMS$[3]=""
3690 GOSUB UPDATE_INVENTORY
3700 REMOVE (WOE23_DEV,KEY=K$,DOM=3710)
3710 GOTO 3630
3720 REMOVE (WOE15_DEV,KEY=A0$(1,11)+"000",DOM=3730)
3730 READ (WOE25_DEV,KEY=A0$(1,11),DOM=3740)
3740 LET K2$=KEY(WOE25_DEV,END=3780)
3750 IF K2$(1,11)<>A0$(1,11) THEN GOTO 3780
3760 REMOVE (WOE25_DEV,KEY=K2$)
3770 GOTO 3740
3780 PRECISION IVS01_PRECISION
3790 WRITE (WOE01_DEV,KEY=A0$(1,11))IOL=WOE01A
3800 REMOVE (WOE04_DEV,KEY=K0$,DOM=3810)
3810 GOTO 1100
4000 REM " --- End"
4010 IF GL$="Y" THEN CALL "GLC.CA",STATUS
4100 GOTO 9900
5600 REM " --- Update Inventory Information "
5610 UPDATE_INVENTORY: 
5620 CALL "IVC.UA",ACTION$,IVFILES[ALL],PARAMS[ALL],PARAMS$[ALL],ITEMS$[ALL],REFS$[ALL],REFS[ALL],STATUS
5630 IF STATUS THEN GOTO 9900
5640 RETURN 
6600 REM " --- Item Distribution Code"
6620 DIM Q8$(80,"0")
6640 FIND (ARM10_DEV,KEY=N0$+"D"+C1$(13,2),DOM=6690)*,Q8$(1)
6660 LET Y2$=Q8$(61,10)
6690 RETURN 
6700 REM " --- Remove Schedule Detail Records"
6710 READ (WOM06_DEV,KEY=A0$(1,2)+A0$(5,7),DOM=6720)
6720 LET K22$=KEY(WOM06_DEV,END=6790)
6730 IF POS(A0$(1,2)+A0$(5,7)=K22$)<>1 THEN GOTO 6790
6740 REMOVE (WOM06_DEV,KEY=K22$)
6750 REMOVE (WOM05_DEV,KEY=K22$(1,2)+K22$(13,6)+K22$(3,10),DOM=6760)
6760 GOTO 6720
6790 RETURN 
6900 REM " --- Standard G/L Posting Routine"
6910 GLPOST: 
6920 IF GL$<>"Y" THEN GOTO 6990
6950 CALL "GLC.AA",GLM01_DEV,GLT04_DEV,GLT05_DEV,ACCOUNT$,WHEN$,REF1$,REF2$,REF3$,MEMO$,AMOUNT,UNITS,STATUS
6990 RETURN 
8000 REM " --- Functions"
8010 DEF FNA$(Q$,Q2$)=STR(MOD((ASC(Q$)-32)*POS(" "<>Q2$(2,1)),100):"00")
8020 DEF FNB$(Q1$)=FNA$(Q1$(2),Q1$)+"/"+FNA$(Q1$(3),Q1$)+"/"+FNA$(Q1$(1),Q1$)
8040 DEF FND$(Q$)=CHR(FNYY_YEAR(Q$(5,2))+32)+CHR(NUM(Q$(1,2))+32)+CHR(NUM(Q$(3,2))+32)
8080 DEF FNP$(Q$)=CVS(Q$,2)
8145 REM " --- FNYY21_YY$ Un-Convert 21st Century 2-Char Year to 2-Char Year"
8150 DEF FNYY21_YY$(Q1$)
8155 LET Q3$=" 01234567890123456789",Q1$(1,1)=Q3$(POS(Q1$(1,1)=" 0123456789ABCDEFGHIJ"))
8160 RETURN Q1$
8165 FNEND
8170 REM " --- FNYY_YEAR Convert 2-Char Year to 21st Century Numeric Year"
8175 DEF FNYY_YEAR(Q1$)
8180 LET Q=NUM(FNYY21_YY$(Q1$)); IF Q<50 THEN LET Q=Q+100
8185 RETURN Q
8190 FNEND
8200 REM " --- FNF$ AlphaNumeric Masking Function"
8210 DEF FNF$(Q1$,Q2$)
8220 IF Q2$="" THEN LET Q2$=FILL(LEN(Q1$),"0")
8230 RETURN STR(-NUM(Q1$,ERR=ALPHA_FNF):Q2$,ERR=ALPHA_FNF)
8240 ALPHA_FNF: 
8245 LET Q=1,Q0=0
8250 WHILE LEN(Q2$(Q))
8255 IF POS(Q2$(Q,1)="-()") THEN LET Q0=Q0+1 ELSE LET Q2$(Q,1)="X"
8260 LET Q=Q+1
8265 WEND
8270 IF LEN(Q1$)>LEN(Q2$)-Q0 THEN LET Q1$=Q1$(1,LEN(Q2$)-Q0)
8280 RETURN STR(Q1$:Q2$)
8290 FNEND
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9800 REM " --- Display Parameter record error"
