0660 LET IVM17_DEV=CHANNELS[30],IVT02_DEV=CHANNELS[31]
0665 LET ARE03_DEV=CHANNELS[32],ARE13_DEV=CHANNELS[33],GLM01_DEV=CHANNELS[34]
0670 LET GLT05_DEV=CHANNELS[35],GLT04_DEV=CHANNELS[36],IVM04_DEV=CHANNELS[37]
0675 LET IVFILES[1]=IVM01_DEV,IVFILES[2]=IVM02_DEV,IVFILES[4]=IVM04_DEV
0680 LET IVFILES[7]=IVM07_DEV,IVFILES[12]=IVM12_DEV,IVFILES[17]=IVM17_DEV
0685 LET IVFILES[41]=IVT01_DEV,IVFILES[42]=IVT02_DEV,IVFILES[44]=IVT04_DEV
0690 LET IVFILES[0]=SYS01_DEV,IVFILES[8]=IVM08_DEV
0700 REM " --- Background"
0710 PRINT @(0,3),'CE',
0800 REM " --- Options"
0810 LET V4$="Are You Ready to Update?"
