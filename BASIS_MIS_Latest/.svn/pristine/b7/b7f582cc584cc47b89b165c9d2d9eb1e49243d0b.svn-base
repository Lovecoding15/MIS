endUserList.from=FROM SNMEU

endUserList.where=WHERE FIRM_ID\='|FIRMID|' AND CUSTOMER_NBR\='|CUSTOMERNBR|'

unlinkedSerialNbrs.select=SELECT T1.SERIAL_NBR

unlinkedSerialNbrs.from=FROM SNM01 T1

unlinkedSerialNbrs.leftOuterJoin=LEFT OUTER JOIN SNMEL T2 ON T1.SERIAL_NBR\=T2.SERIAL_NBR

unlinkedSerialNbrs.where=WHERE T2.SERIAL_NBR IS NULL AND T1.ACTIVE_FLAG\='Y' AND T1.SERIAL_NBR NOT LIKE '91%' AND T1.FIRM_ID \='|FIRMID|' AND T1.CUSTOMER_NBR\='|CUSTOMERNBR|'

unlinkedSerialNbrs.orderby=ORDER BY T1.SERIAL_NBR

