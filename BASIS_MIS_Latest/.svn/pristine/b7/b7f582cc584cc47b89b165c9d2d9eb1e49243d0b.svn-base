endUserDataOne.select=SELECT T2.SERIAL_NBR, T1.* , T3.INDUSTRY_NAME

endUserDataOne.from=FROM SNMEU T1

endUserDataOne.outerJoin1=LEFT OUTER JOIN SNMEL T2 ON T1.END_USER_NBR \= T2.END_USER_NBR

endUserDataOne.outerJoin2=LEFT OUTER JOIN ECEIN T3 ON T1.INDUSTRY \= T3.INDUSTRY_ID

endUserDataOne.where=WHERE T1.FIRM_ID\=? AND T1.CUSTOMER_NBR\=? AND T2.SERIAL_NBR IS NULL

