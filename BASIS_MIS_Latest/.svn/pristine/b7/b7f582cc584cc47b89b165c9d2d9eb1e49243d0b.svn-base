endUserTemplate.select=SELECT T1.SERIAL_NBR, T1.ACTIVE_FLAG, T1.FIRM_ID, T1.CUSTOMER_NBR 

endUserTemplate.from=FROM SNM01 T1

endUserTemplate.leftOuterJoin=LEFT OUTER JOIN SNMEL T2 ON T1.SERIAL_NBR \= T2.SERIAL_NBR

endUserTemplate.where=WHERE T2.SERIAL_NBR IS NULL AND T1.ACTIVE_FLAG\='Y' AND T1.FIRM_ID \=? AND T1.CUSTOMER_NBR\=?

