endUserDataTwo.outerJoin1=LEFT OUTER JOIN ECEIN T4 ON T1.INDUSTRY \= T4.INDUSTRY_ID

endUserDataTwo.where=WHERE T1.FIRM_ID\=? AND T1.CUSTOMER_NBR\=? AND T3.ACTIVE_FLAG \= 'Y'

endUserDataTwo.orderby=ORDER BY T2.SERIAL_NBR

