0010 REM "SHE - UPS Zone Determination (International Expedited/Letter)"
0020 REM "Program SHCAZG"
0022 REM "+-----------------------------------------------------------+"
0024 REM "| ADD+ON Software, Inc. Advantage/V Version 6.0.2 - 01Feb97 |"
0026 REM "|      Copyright (c) 1996 Open Systems Holdings Corp.       |"
0028 REM "|                  All Rights Reserved                      |"
0030 REM "+-----------------------------------------------------------+"
0031 REM "01Feb97 CAJ: Modified for 1997 SpeedShip Update"
0040 REM 
0080 SETERR 9000
0085 SETESC 9000
0090 ENTER A0$,A1$,W0$,W1$,W[ALL],ZIPLENGTH,ZIPCODE$,ZONE$,RESERVED1$,RESERVED2$,ZONEFILE$,SPEEDSHIP$
0100 REM " --- Open/Lock Files"
0110 LET FILES=1
0120 DIM FILES$[FILES],OPTIONS$[FILES],CHANNELS[FILES]
0130 LET FILES$[1]=ZONEFILE$
0160 CALL "SYC.DA",1,1,FILES,FILES$[ALL],OPTIONS$[ALL],CHANNELS[ALL],BATCH,STATUS
0170 IF STATUS THEN GOTO 9900
0180 LET SHM28_DEV=CHANNELS[1]
0200 REM " --- IOLIST's"
0210 SHM28A: IOLIST Z0$,*,ZONE
0400 REM " --- Parameters"
0410 LET FIRM$=A0$(1,2),CARRIER$=W1$(11,1),WAREHOUSE$=W1$(59,2)
3000 REM " --- Destination Lookup"
3010 LET V5$=WAREHOUSE$+CARRIER$
3020 CALL "SHC.LI",SHM28_DEV,"I",V5$,V$
3050 IF LEN(V$)<>0 THEN GOTO 3500
3060 LET ZONE$="EXIT"
3070 GOTO 4000
3500 REM " --- Retrieve Zone using lookup selection"
3510 IF LEN(V$)<40 THEN LET V$=V$+FILL(40),V$=V$(1,40)
3520 FIND (SHM28_DEV,KEY=FIRM$+V5$+V$,DOM=3000)IOL=SHM28A
3530 LET ZONE$=STR(ZONE:"000")
4000 REM " --- All done"
4090 GOTO 9900
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9900 REM " --- Return to calling program"
9910 CLOSE (SHM28_DEV,ERR=9950)
9950 EXIT 
9999 END
