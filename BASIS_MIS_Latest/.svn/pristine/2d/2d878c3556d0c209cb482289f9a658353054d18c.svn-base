/* Icon Font */
@font-face {
  font-family: 'Material Icons';
  src: url('/files/fonts/MaterialIcons-Regular.woff2') format('woff2'), url('/files/fonts/MaterialIcons-Regular.woff') format('woff'), url('/files/fonts/MaterialIcons-Regular.ttf') format('truetype');
  font-weight: 400;
  font-style: normal;
}
.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;
  /* Preferred icon size */
  display: inline-block;
  line-height: 1;
  text-transform: none;
  letter-spacing: normal;
  word-wrap: normal;
  white-space: nowrap;
  direction: ltr;
  /* Support for all WebKit browsers. */
  -webkit-font-smoothing: antialiased;
  /* Support for Safari and Chrome. */
  text-rendering: optimizeLegibility;
  /* Support for Firefox. */
  -moz-osx-font-smoothing: grayscale;
  /* Support for IE. */
  font-feature-settings: 'liga';
}
/* All controls base definition */
.BBjControl,
.BBjMsgBox-message {
  font-family: BloggerSans, "Lucida Grande", Lucida, Verdana, sans-serif !important;
  font-weight: 500 !important;
  font-size: 15px !important;
  outline-color: transparent !important;
}
/* All top-level windows */
.BBjTopLevelWindow {
  box-shadow: 0 1px 5px rgba(0, 0, 0, 0.1);
  margin: 20px 5px 5px 5px !important;
  border: 1px solid #c7ccd1 !important;
  border-radius: 0 0 6px 6px !important;
}
/* All buttons */
.BBjButton,
.BBjMsgBox-button {
  font-family: Akrobat, BloggerSans, "Lucida Grande", Lucida, Verdana, sans-serif !important;
  font-weight: 600 !important;
  font-size: 14px !important;
  color: #3a5978;
  /* Download buttons icon */
  /* Launch buttons icon */
}
.BBjButton[class*=" download"],
.BBjMsgBox-button[class*=" download"] {
  padding-left: 10px !important;
}
.BBjButton[class*=" download"]::before,
.BBjMsgBox-button[class*=" download"]::before {
  content: "\e2c4";
  font-family: "Material Icons" !important;
  font-size: 18px !important;
  padding-top: 2px;
  float: left;
  /*position: absolute;
	top: 3px;
	left: 5px;*/
  color: #55595e !important;
}
.BBjButton[class*=" download"] .html-face,
.BBjMsgBox-button[class*=" download"] .html-face {
  display: inline-block;
  padding-top: 2px;
}
.BBjButton[class*=" launch"],
.BBjMsgBox-button[class*=" launch"] {
  padding-left: 10px !important;
}
.BBjButton[class*=" launch"]::before,
.BBjMsgBox-button[class*=" launch"]::before {
  content: "\e0b2";
  font-family: "Material Icons" !important;
  font-size: 18px !important;
  padding-top: 2px;
  float: left;
  /*position: absolute;
	top: 3px;
	left: 5px;*/
  color: #55595e !important;
}
.BBjButton[class*=" launch"] .html-face,
.BBjMsgBox-button[class*=" launch"] .html-face {
  display: inline-block;
  padding-top: 2px;
}
.BBjMsgBox-button {
  min-width: 75px;
  padding-top: 4px;
  font-size: 15px !important;
}
/* All static text */
.BBjStaticText,
.BBjMsgBox-message {
  color: #55595e;
}
/* Editboxes with placeholders */
.BBjEditBox {
  color: #56738f;
  font-family: monospace !important;
  font-weight: 500 !important;
  padding-left: 4px;
  text-shadow: none !important;
  /* Remove the text's drop shadow for all states */
  /* Placeholder text for EditBoxes */
}
.BBjEditBox:active,
.BBjEditBox:hover,
.BBjEditBox:active {
  text-shadow: none !important;
}
.BBjEditBox::placeholder {
  font-weight: 300 !important;
  color: #bfbfbf !important;
}
/* The title for each window and message boxes */
.headertext,
.headerText2,
.headerText3,
.headerText4,
.headerText5,
.BBjMsgBox-title {
  font-family: NotoSerif, BloggerSans, "Lucida Grande", Lucida, Verdana, sans-serif !important;
  font-weight: 500 !important;
  font-size: 18px !important;
  letter-spacing: 0.5px;
  color: #f5faff !important;
  background: #1d73c9 !important;
  text-shadow: 0 0 2px #3a5978 !important;
}
.headertext:not(.BBjMsgBox-title),
.headerText2:not(.BBjMsgBox-title),
.headerText3:not(.BBjMsgBox-title),
.headerText4:not(.BBjMsgBox-title),
.headerText5:not(.BBjMsgBox-title),
.BBjMsgBox-title:not(.BBjMsgBox-title) {
  left: 0 !important;
  top: 0 !important;
  border-bottom: 1px solid #3a5978 !important;
  line-height: 28px !important;
  width: 635px !important;
}
/* Groupboxes and their borders/legends */
.BBjGroupBox {
  border-color: #c7ccd1 !important;
  border-radius: 3.1px !important;
}
.BBjGroupBox legend {
  font-family: Akrobat, BloggerSans !important;
  font-weight: 600 !important;
  font-size: 16px !important;
  margin-left: 5px;
  color: #1d73c9 !important;
}
/* Timer countdown progress bar */
.BBjProgressBar {
  background-color: #fbb451 !important;
}
.BBjProgressBar .BBjProgressBar-text {
  text-shadow: 0 1px 1px rgba(255, 255, 255, 0.5);
  padding-top: 1px !important;
}
/* Grid controls */
.BBjGrid .BBjGrid-columnHeaderCell {
  font-weight: 700;
  line-height: 24px;
  background: #e0e6eb !important;
  box-shadow: inset 0 1px 1px rgba(255, 255, 255, 0.75);
}
