}
/* Message Box tweaks */
.BBjMsgBox .BBjMsgBox-title,
.BBjMsgBox .dialogMiddleCenter {
  border-radius: 0 !important;
  box-shadow: none !important;
}
.BBjMsgBox .dialogTopCenterInner {
  margin-top: -2px;
}
.BBjMsgBox > div > table {
  box-shadow: 0 0 40px #bfbfbf !important;
}
/* Busy Indicator */
.BBjWindowManager-busyIndicator {
  background: rgba(255, 255, 255, 0.5) !important;
  border: 2px solid rgba(25, 102, 179, 0.75) !important;
  box-shadow: 0 0 40px #bfbfbf !important;
  padding: 0 20px 20px 20px !important;
  color: #3a5978;
  /* Do NOT use the BloggerSans font, or it causes resizing problems for the BUIIndicator */
  font-family: Arial, sans-serif !important;
}
