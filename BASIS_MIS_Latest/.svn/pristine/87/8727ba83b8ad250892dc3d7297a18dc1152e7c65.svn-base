//#charset: windows-1252

VERSION "4.0"

WINDOW 101 "Backup Licenses" 100 100 1000 625
BEGIN
    NOT CLOSEBOX
    EVENTMASK 0
    NOT MAXIMIZABLE
    NAME "backupLicense"
    NOT SIZABLE
    BUTTON 100, "Exit", 900, 5, 90, 25
    BEGIN
        NAME "exitBtn"
    END

    GRID 102, "", 10, 35, 980, 580
    BEGIN
        BACKGROUNDCOLOR RGB(255,255,255)
        COLUMNHEAD 20, 104
        COLUMNS 9
        FOREGROUNDCOLOR RGB(0,0,0)
        GRIDCOLTITLE 0, "Firm"
        GRIDCOLTITLE 1, "Cust#"
        GRIDCOLTITLE 2, "Customer Name"
        GRIDCOLTITLE 3, "Contract"
        GRIDCOLTITLE 4, "Expiration"
        GRIDCOLTITLE 5, "Prod SN"
        GRIDCOLTITLE 6, "Prod Users"
        GRIDCOLTITLE 7, "Backup SN"
        GRIDCOLTITLE 8, "Backup Users"
        GRIDCOLWIDTH 0, 40
        GRIDCOLWIDTH 1, 55
        GRIDCOLWIDTH 2, 180
        GRIDCOLWIDTH 3, 55
        GRIDCOLWIDTH 4, 80
        GRIDCOLWIDTH 5, 100
        GRIDCOLWIDTH 6, 80
        GRIDCOLWIDTH 7, 100
        GRIDCOLWIDTH 8, 80
        HORIZLINES
        HSCROLLBAR
        MAXCOLS 2147483647
        NAME "theGrid"
        ROWS 24
        USERSIZE
        VERTLINES
        VSCROLLBAR
    END

    STATICTEXT 103, "Double Click on a Serial Number to load the serial number.", 10, 7, 300, 20
    BEGIN
        NAME "directionText"
        NOT WORDWRAP
    END

END

