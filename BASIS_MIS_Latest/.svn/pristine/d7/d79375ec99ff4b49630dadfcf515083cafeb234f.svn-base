0010 REM "APR - VENDOR LABEL PRINTING II"
0020 REM "Program APR.KB"
0022 REM "+-----------------------------------------------------------+"
0024 REM "| ADD+ON Software, Inc. Advantage/V Version 6.0.0 - 15May96 |"
0026 REM "|      Copyright (c) 1996 Open Systems Holdings Corp.       |"
0028 REM "|                  All Rights Reserved                      |"
0030 REM "+-----------------------------------------------------------+"
0040 REM 
0080 SETESC 9000
0090 SETERR 9000
0100 REM "OPEN/LOCK FILES"
0200 REM "IOLISTS"
0210 APM01A: IOLIST A0$,A1$
0400 REM "PARAMETERS"
0500 REM "INIT DATA"
0510 LET X8$="First"; IF W2<4 THEN LET X8=W2 ELSE LET X8=4
0700 REM "BACKGROUND"
0800 REM "OPTIONS"
0805 CALL "SYC.YN",0,"Do You Wish To Print A Test Pattern",3,V$,V3
0825 PRINT @(0,13),'CL',
0830 IF V3=4 THEN GOTO 9900
0840 ON POS(V$="YN") GOTO 0800,0850,0900
0850 DIM W0$(W0*W1*4,"X")
0860 LET X0=1
0870 GOSUB 5500
0880 GOTO 0800
0900 REM "DETERMINE START"
0930 CALL "SYC.NB","Printing",30,COLUMN
0980 ON POS(P7$="NAZ") GOTO 0910,1000,1500,1800
1000 REM "NUMERIC SEQUENCE"
1020 READ (APM01_DEV,KEY=N0$+P6$,DOM=1030)IOL=APM01A
1025 GOTO 1130
1100 REM "MAIN READ"
1120 READ (APM01_DEV,END=4000)IOL=APM01A
1130 IF A0$(1,2)<>N0$ THEN GOTO 4000
1135 IF P9$="N" THEN GOTO 1200
1140 IF P8$<>"" THEN IF A0$(3,6)>P8$ THEN GOTO 4000
1200 REM "SETUP"
1280 GOSUB 5000
1290 GOTO 1100
1500 REM "ALTERNATE SEQUENCE"
1510 LET P7$="A"
1520 READ (APM04_DEV,KEY=N0$+P7$+P6$,DOM=1530)
1600 REM "MAIN READ"
1620 LET K$=KEY(APM04_DEV,END=4000)
1640 IF K$(1,3)<>N0$+P7$ THEN GOTO 4000
1645 IF P9$="N" THEN GOTO 1660
1650 IF P8$<>"" THEN IF K$(4,LEN(P8$))>P8$ THEN GOTO 4000
1660 READ (APM04_DEV)
1680 FIND (APM01_DEV,KEY=N0$+K$(14),DOM=1600)IOL=APM01A
1700 REM "SETUP"
1780 GOSUB 5000
1790 GOTO 1600
1800 REM "ZIP CODE SEQUENCE"
1810 LET P7$="B"
1820 READ (APM04_DEV,KEY=N0$+P7$+P6$,DOM=1830)
1900 REM "MAIN READ"
1920 LET K$=KEY(APM04_DEV,END=4000)
1940 IF K$(1,3)<>N0$+P7$ THEN GOTO 4000
1945 IF P9$="N" THEN GOTO 1960
1950 IF P8$<>"" THEN IF K$(4,P[2])>P8$ THEN GOTO 4000
1960 READ (APM04_DEV)
1980 FIND (APM01_DEV,KEY=N0$+K$(13),DOM=1900)IOL=APM01A
2000 REM "SETUP"
2080 GOSUB 5000
2090 GOTO 1900
4000 REM "TOTALS"
4020 GOSUB 5500
4100 GOTO 9900
5000 REM "PREPARE ADDRESS"
5100 REM "ASSEMBLE"
5120 LET X3=0
5130 LET X3=X3+1
5140 IF X3>W3 THEN RETURN 
5150 IF X0>=W0 THEN GOSUB 5500
5160 LET X0=X0+1
5180 PRINT @(COLUMN,11),A1$(1,30),
5190 LET W1$=A1$(31,72)+A1$(103,P[2])
5194 CALL "SYC.AA",W1$,24,3,P[2],30
5200 LET W0$(W1*(X0-1)+1,W1)=A1$(1,30)
5210 LET W0$(W1*(W0+X0-1)+1,W1)=W1$(1,30)
5220 LET W0$(W1*(2*W0+X0-1)+1,W1)=W1$(31,30)
5230 LET W0$(W1*(3*W0+X0-1)+1,W1)=W1$(61,30)
5290 GOTO 5130
5500 REM "PRINT"
5520 IF X0=0 THEN RETURN 
5530 IF X8$="First" THEN FOR X=1 TO W2; PRINT (7)""; NEXT X; LET X8$=""
5540 FOR X=0 TO X8-1
5560 PRINT (7)@(0+W9),W0$(X*W0*W1+1,W0*W1)
5580 NEXT X
5600 IF W2>4 THEN PRINT (7)W9$(1,W2-5)
5620 LET X0=0,W0$(1)=""
5690 RETURN 
8000 REM "FUNCTIONS"
9000 REM " --- Standard Error Routine (15May95)"
9010 IF ERR=127 THEN GOTO 9300
9020 CALL "SYC.EA",ERR=9090,EXIT_CTRL,PGM(-2),ERR,TCB(5),E$,E1,E2
9030 IF E1=1 THEN GOTO 9100
9040 IF E1=3 THEN GOTO 9200
9080 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9090 ESCAPE
9100 REM " --- Retry"
9190 RETRY
9200 REM " --- Return"
9210 SETERR 9000
9290 GOTO 9900
9300 REM " --- Standard Escape Routine"
9310 CALL "SYC.ES",ERR=9350,PGM(-2),TCB(8),E$,E2,V3
9320 IF V3<>127 THEN GOTO 9390
9330 PRINT @(0,E2),'CL','CURSOR'("ON"),E$,'LF'
9350 ESCAPE
9390 RETURN 
9900 RUN "SYS.AA"
9999 END
