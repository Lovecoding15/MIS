VERSION "4.0"

WINDOW 1000 "" 10 40 0560 0132
BEGIN
    NAME "win_apm_invimage"
    MANAGESYSCOLOR
    KEYBOARDNAVIGATION
    DIALOGBEHAVIOR
    EVENTMASK 1136656524
    INVISIBLE
    FONT "Dialog" 8
    ENTERASTAB
    
    STATICTEXT 02001, "Vendor ID:", 39, 12, 88, 17
    BEGIN
        NOT OPAQUE
        JUSTIFICATION 32768
        NAME "txt_vendor_id"
        FONT "Dialog" 8
    END
    
    INPUTE 03001, "", 130, 10, 61, 19
    BEGIN
        NAME "ine_vendor_id"
        CLIENTEDGE
        PASSENTER
        HIGHLIGHT
        PADCHARACTER 0
        MASK "UUUUUU"
        FONT "Dialog" 8
    END
    
    STATICTEXT 04001, "", 226, 12, 200, 17
    BEGIN
        NOT OPAQUE
        NOT WORDWRAP
        NAME "dis_vendor_id"
        FOREGROUNDCOLOR RGB(0,0,96)
        FONT "Dialog" 8
    END
    MENUBUTTON 20001, "", 191, 10, 30, 19
    BEGIN
        NAME "tbnf_vendor_id"
    END
    
    STATICTEXT 02002, "AP Invoice Number:", 0, 33, 127, 17
    BEGIN
        NOT OPAQUE
        JUSTIFICATION 32768
        NAME "txt_ap_inv_no"
        FONT "Dialog" 8
    END
    
    INPUTE 03002, "", 130, 31, 96, 19
    BEGIN
        NAME "ine_ap_inv_no"
        CLIENTEDGE
        PASSENTER
        HIGHLIGHT
        PADCHARACTER 0
        MAXLENGTH 10
        FONT "Dialog" 8
    END
    
    STATICTEXT 02003, "Sequence Number:", 0, 54, 127, 17
    BEGIN
        NOT OPAQUE
        JUSTIFICATION 32768
        NAME "txt_sequence_num"
        FONT "Dialog" 8
    END
    
    INPUTE 03003, "", 130, 52, 44, 19
    BEGIN
        NAME "ine_sequence_num"
        CLIENTEDGE
        PASSENTER
        HIGHLIGHT
        PADCHARACTER 0
        MASK "00"
        FONT "Dialog" 8
    END
    
    STATICTEXT 02004, "Text:", 79, 75, 48, 17
    BEGIN
        NOT OPAQUE
        JUSTIFICATION 32768
        NAME "txt_doc_url"
        FONT "Dialog" 8
    END
    
    INPUTE 03004, "", 130, 73, 400, 19
    BEGIN
        NAME "ine_doc_url"
        CLIENTEDGE
        PASSENTER
        HIGHLIGHT
        PADCHARACTER 0
        MAXLENGTH 80
        FONT "Dialog" 8
    END
END

